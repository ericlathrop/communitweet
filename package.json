{
	"name": "communitweet",
	"version": "0.1.1",
	"description": "Communitweet creates a central Twitter account for your community by automatically retweeting community members.",
	"main": "bin/communitweet.js",
	"bin": "bin/communitweet.js",
	"preferGlobal": true,
	"scripts": {
		"lint-js": "bash -c \"jshint bin/*.js\"",
		"build": "npm run lint-js"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ericlathrop/retweet-club-members.git"
	},
	"keywords": [
		"twitter",
		"bot",
		"retweet",
		"club"
	],
	"author": "Eric Lathrop <eric@ericlathrop.com> (http://ericlathrop.com)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ericlathrop/retweet-club-members/issues"
	},
	"homepage": "https://github.com/ericlathrop/retweet-club-members",
	"devDependencies": {
		"jshint": "~2.5.2"
	},
	"jshintConfig": {
		"curly": true,
		"eqeqeq": true,
		"forin": true,
		"freeze": true,
		"immed": true,
		"indent": 4,
		"latedef": "nofunc",
		"newcap": true,
		"noarg": true,
		"node": true,
		"noempty": true,
		"nonbsp": true,
		"nonew": true,
		"quotmark": "double",
		"unused": "strict",
		"trailing": true,
		"browser": true,
		"devel": true,
		"globalstrict": true,
		"globals": {
		}
	},
	"dependencies": {
		"twit": "~1.1.15"
	}
}
